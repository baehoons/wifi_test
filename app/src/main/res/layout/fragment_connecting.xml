<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".ConnectingFragment">

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView" app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
        app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="16dp" android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/textView8"/>
    <TextView
        android:text="BSSID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView2"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="50dp" android:layout_marginTop="84dp"
        app:layout_constraintTop_toTopOf="parent"/>
    <TextView
        android:text="Level"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/textView2" app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="50dp" android:layout_marginTop="50dp"/>
    <TextView
        android:text="Frequency"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        android:layout_marginTop="50dp" app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="50dp"/>
    <TextView
        android:text="Capabilities"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView5"
        android:layout_marginTop="50dp"
        app:layout_constraintTop_toBottomOf="@+id/textView4" app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="50dp"/>
    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView6" app:layout_constraintStart_toEndOf="@+id/textView4"
        android:layout_marginStart="56dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
        app:layout_constraintTop_toBottomOf="@+id/textView7" android:layout_marginTop="50dp"
        app:layout_constraintHorizontal_bias="0.488"/>
    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView7" app:layout_constraintStart_toEndOf="@+id/textView3"
        android:layout_marginStart="88dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
        app:layout_constraintTop_toBottomOf="@+id/textView8" android:layout_marginTop="50dp"
        app:layout_constraintHorizontal_bias="0.491"/>
    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView8" app:layout_constraintStart_toEndOf="@+id/textView2"
        android:layout_marginStart="84dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
        android:layout_marginTop="84dp" app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintHorizontal_bias="0.488"/>
    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView10" app:layout_constraintStart_toEndOf="@+id/textView5"
        android:layout_marginStart="44dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
        app:layout_constraintTop_toBottomOf="@+id/textView6" android:layout_marginTop="50dp"/>

    <Button
        android:id="@+id/button"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:text="Connect"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>